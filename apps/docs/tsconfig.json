{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["@docusaurus/module-type-aliases"],
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "include": ["src/**/*", "docusaurus.config.ts", "sidebars.ts"],
  "exclude": ["node_modules", "build", ".docusaurus"]
}
