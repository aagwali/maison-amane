{
  "name": "@maison-amane/server",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./domain": "./src/domain/index.ts",
    "./infrastructure": "./src/infrastructure/index.ts",
    "./composition": "./src/composition/index.ts"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/main.ts",
    "start": "tsx --env-file=.env src/main.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@effect/platform": "^0.77.1",
    "@effect/platform-node": "^0.73.1",
    "@maison-amane/api": "workspace:*",
    "@maison-amane/shared-kernel": "workspace:*",
    "amqplib": "^0.10.9",
    "effect": "^3.13.1",
    "mongodb": "^6.12.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^22.5.1",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
